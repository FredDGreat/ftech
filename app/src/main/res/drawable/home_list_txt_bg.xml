<?xml version="1.0" encoding="utf-8"?>
<selector
 	 xmlns:android="http://schemas.android.com/apk/res/android"   
 >
	<item 
	    android:state_focused="true"
	    android:state_pressed="true"
	   >
		<shape
		    android:dither="true"
		    android:shape="rectangle"
		>
		    <solid
		        android:color="#99999999"
		    />
			<stroke
				android:width="1dp"
				android:color="#99d2d2d3"
				/>
		</shape>
	</item>
	<item 
	    android:state_focused="false"
	    android:state_pressed="true"
    >
		<shape
		    android:dither="true"
		    android:shape="rectangle"
		>
		    <solid
		        android:color="#99999999"
		    />
			<stroke
				android:width="1dp"
				android:color="#99d2d2d3"
				/>
		</shape>
    </item>
	<item >
		<shape
		    android:dither="true"
		    android:shape="rectangle"
		>
     		<solid android:width="1dp" android:color="#00ffffff" />
			<stroke
				android:width="1dp"
				android:color="#99d2d2d3"
				/>
		</shape>
	</item>
</selector>