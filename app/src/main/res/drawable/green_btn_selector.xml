<?xml version="1.0" encoding="utf-8"?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android"
	>
	<item
		android:state_focused="true"
		android:state_pressed="true"
		>
		<shape
			android:dither="true"
			android:shape="rectangle"
			>
			<solid
				android:color="#97cc87"
				/>
		</shape>
	</item>
	<item
		android:state_focused="false"
		android:state_pressed="true"
		>
		<shape
			android:dither="true"
			android:shape="rectangle"
			>
			<solid
				android:color="#97cc87"
				/>
		</shape>
	</item>
	<item >
		<shape
			android:dither="true"
			android:shape="rectangle"
			>
			<gradient
				android:startColor="#29800f"
				android:centerColor="#4ba330"
				android:endColor="#29830f"
				/>
		</shape>
	</item>
</selector>